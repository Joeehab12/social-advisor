<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title> Human Social Advisor </title>
    <!--Import Google Icon Font-->
    <!-- <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"> -->
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="css/materialize.min.css"  media="screen,projection"/>
    <style>
    @font-face {
        font-family: 'Material Icons';
        font-style: normal;
        font-weight: 400;
        src: url(fonts/material-icons.woff2) format('woff2');
    }

    .material-icons {
        font-family: 'Material Icons';
        font-weight: normal;
        font-style: normal;
        font-size: 24px;
        line-height: 1;
        letter-spacing: normal;
        text-transform: none;
        display: inline-block;
        white-space: nowrap;
        word-wrap: normal;
        direction: ltr;
        -moz-font-feature-settings: 'liga';
        -moz-osx-font-smoothing: grayscale;
    }
    .mylink{
        height:64px;
    }
    #chipsContainer{
        width:60%;
        margin :auto;
    }
    #center_div{
        width: 100%;
        align-items: center;
        display: flex;
        padding: 0px 16px;
    }
    .tagDiv{
        width: 6%;
        min-width: 81px;
        display: flex;
        margin: auto;

    }
    .myimg{
        margin-top:7px;
        margin-left:7px;
    }

    #form1{
        margin-left:200px;
    }
    #current-user{
        display: inline-block;
        width:160px;
    }
    .active-upvote{
        color: #339933;
    }

    .btn-vote:hover{
        color: #339933;
    }



    #autocomplete-input:hover{
        outline: none;
        box-shadow: 0px 0px 5px #d3d3d3;
        background-color: #fff;
        border:1px solid #d3d3d3;
    }

    #autocomplete-input:focus{
        outline: none;
        box-shadow: 0px 0px 5px #d3d3d3;
        background-color: #fff;
        border:1px solid #d3d3d3;
    }
    #autocomplete-input{
        height: 25px;
    }

    #search{
        position: relative;
        margin-right: 300px;
    }
    #autocomplete-input{
        width:80%;
    }
    .side-nav li a {
        text-decoration:none;
    }
    #searchResults2{
        background-color:#666666;
    }
    #searchResults{
        background-color:#666666;
    }
    .right li a{
        padding-right:2em;
        text-decoration:none
    }

    #user-img{
        position: absolute;
        margin-top: 15px;
        margin-left: 10px;
        border-radius: 100px;
    }
    .input-field input[type=date]:focus + label,
    .input-field input[type=text]:focus + label,
    .input-field input[type=title]:focus + label,
    .input-field input[type=description]:focus + label {
        color: #e91e63;
    }

    .input-field input[type=date]:focus,
    .input-field input[type=text]:focus,
    .input-field input[type=title]:focus,
    .input-field input[type=description]:focus {
        border-bottom: 2px solid #e91e63;
        box-shadow: none;
    }


    </style>
</head>
<body>
    <nav>
        <div class="nav-wrapper">
            <a href="#" class="brand-logo">mySocial</a>
            <a href = "#"><img src = "Webp.net-resizeimage (1).png" href="#" class = "logo"></img></a>

            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li>


                    <div class="row" id="topbarsearch">
                        <div id ="search" class="input-field col s6 s12 white-text">
                            <i class="white-text material-icons prefix">search</i>
                            <input type="text" placeholder="Search for a certain keyword..." id="autocomplete-input" class="autocomplete black-text" >
                        </div>
                    </div>

                </li>

            </ul>

        </div>
    </nav>

    <!-- <div id = "center_div">
        <div class="tagDiv">

        </div>


    </div> -->
    <ul id = "results" class="collapsible" data-collapsible="accordion">

    </ul>

    <script type="text/javascript" src="scripts/jquery-3.2.1.min.js"></script>
    <script type = "text/javascript" src="jquery.cookie.js"></script>
    <script type="text/javascript" src="css/js/materialize.min.js"></script>
    <script type="text/javascript" src="scripts/feed.js"></script>
    <script type="text/javascript" src="scripts/logout.js"></script>

</body>
</html>
